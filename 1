class Dial:
    def __init__(self, **kwargs):
        self._number = 50

    @property
    def number(self) -> int:
        return self._number

    @number.setter
    def number(self, number: int) -> None:
        self._number = number

    def turn_right(self) -> int:
        if self.number == 99:
            self.number = 0
            return 0
        if self.number < 99:
            self.number += 1
            return self.number
    

    def turn_left(self) -> int:
        if self.number == 0:
            self.number = 99
            return 99
        if self.number > 0:
            self.number -= 1
            return self.number
if __name__ == '__main__':
    dial = Dial()
    print(dial.number)
    dial.turn_right()
    print(dial.number)
    dial.turn_left()
    dial.turn_left()
    print(dial.number)
    for i in range(200):
        dial.turn_left()
        print(dial.number)
